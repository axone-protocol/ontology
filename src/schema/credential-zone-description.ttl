@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <https://ontology.okp4.space/schema/credential/digital-resource/zone/description> .
@prefix schema: <http://schema.org/> .
@prefix thesaurus: <https://ontology.okp4.space/thesaurus/> .

:ZoneDescriptionCredential a rdfs:Class ;
  rdfs:label "Zone Description Credential"@en ;
  rdfs:comment """
  Zone Description credentials deliver an extensive overviewof the essential information about a zone. This typically comprises details like the zone's title, description, and tags.
  
  The primary objective of this metadata is to provide a broad overview of the zone, facilitating users' comprehension of its purpose and potential use cases.
  """@en .

:hasDescription a rdf:Property ;
  rdfs:label "has description"@en ;
  rdfs:comment """
  A description of the Dataset.
  """@en ;
  schema:domainIncludes :ZoneDescriptionCredential ;
  schema:rangeIncludes xsd:string .

:hasImage a rdf:Property ;
  rdfs:label "has tag"@en ;
  rdfs:comment """
  The image associated with the Dataset. 
  """@en ;
  schema:domainIncludes :ZoneDescriptionCredential ;
  schema:rangeIncludes xsd:anyURI .

:hasTag a rdf:Property ;
  rdfs:label "has tag"@en ;
  rdfs:comment """
  A (free) keyword or term assigned to the Dataset. 
  """@en ;
  schema:domainIncludes :ZoneDescriptionCredential ;
  schema:rangeIncludes xsd:string .

:hasTitle a rdf:Property ;
  rdfs:label "has title"@en ;
  rdfs:comment """
  The title of the Dataset.
  """@en ;
  schema:domainIncludes :ZoneDescriptionCredential ;
  schema:rangeIncludes xsd:string .

:hasTopic a rdf:Property ;
  rdfs:label "has topic"@en ;
  rdfs:comment """
  A topic of the Dataset.
  """@en ;
  schema:domainIncludes :ZoneDescriptionCredential ;
  schema:rangeIncludes thesaurus:topic .


